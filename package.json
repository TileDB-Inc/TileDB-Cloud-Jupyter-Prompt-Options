{
  "name": "tiledb_prompt_options",
  "version": "0.0.0",
  "description": "TileDB notebook extension to prompt user for notebook options",
  "author": "TileDB, Inc.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/TileDB-Inc/TileDB-Cloud-Jupyter-Prompt-Options.git"
  },
  "license": "BSD",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "private": true,
  "scripts": {
    "build": "tsc",
    "build:lab": "rimraf lab-dist && mkdirp lab-dist && cd lab-dist && npm pack ..",
    "build:all": "npm run build && npm run build:lab",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint './src/**/*.{ts,tsx}' --fix",
    "clean": "rimraf lib",
    "watch": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}"
  ],
  "jupyterlab": {
    "extension": true,
    "discovery": {
      "server": {
        "base": {
          "name": "tiledb_prompt_options"
        },
        "managers": [
          "pip"
        ]
      }
    }
  },
  "dependencies": {
    "@jupyterlab/application": "^2.0.0",
    "@jupyterlab/launcher": "^2.0.0",
    "@jupyterlab/mainmenu": "^2.0.0",
    "@jupyterlab/notebook": "^2.0.0",
    "@jupyterlab/coreutils": "^4.2.5",
    "@jupyterlab/services": "^5.2.5",
    "@tiledb-inc/tiledb-cloud": "^0.0.7",
    "base-utils": "^0.0.10",
    "crypto": "^1.0.1",
    "react": "~16.9.0",
    "requests-helper": "^0.1.5"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.21.0",
    "@typescript-eslint/parser": "^2.21.0",
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.9.5",
    "isomorphic-fetch": "^2.2.1",
    "mkdirp": "^0.5.1",
    "prettier": "^2.2.1",
    "rimraf": "^2.6.1",
    "typescript": "^3.1.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2"
  },
  "resolutions": {
    "node-notifier": "^8.0.1"
  }
}
